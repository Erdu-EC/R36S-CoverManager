import {MainView} from "./index.slint";
import {ROOT_GLOBAL} from "./global.slint";

export component App inherits Window {
    preferred-height: ROOT-GLOBAL.window-height;
    preferred-width: ROOT-GLOBAL.window-width;
    title: ROOT-GLOBAL.app-title;

    in property <string> main_dir;

    MainView {
        main_dir: main_dir;
    }
}
